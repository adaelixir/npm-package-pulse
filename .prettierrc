{
  "printWidth": 200,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "semi": false,
  "trailingComma": "none",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "jsxBracketSameLine": false,
  "overrides": [
    {
      "files": "*.tsx",
      "options": {
        "printWidth": 160,
        "attr": "single",
        "jsxBracketSameLine": true
      }
    }
  ]
}
